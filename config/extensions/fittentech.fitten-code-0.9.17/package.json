		"configuration": {
			"title": "FittenCode",
			"properties": {
				"fittencode.disableSpecificInlineCompletion.suffixes": {
					"type": "string",
					"default": "",
					"markdownDescription": "Disable auto-completion for some specific file suffixes by entering them below, and separate each suffix with a semicolon. For instance, type: \"md;py\"",
					"scope": "application"
				},
				"fittencode.inlineCompletion.enable": {
					"type": "boolean",
					"default": true,
					"markdownDescription": "Enable inline code completion.",
					"scope": "application"
				},
				"fittencode.syntaxHighlighting.useVisualStudioCodeColors": {
					"type": "boolean",
					"default": false,
					"markdownDescription": "Use the Visual Studio Code Theme colors for syntax highlighting in the diff viewer. Might not work with all themes. Only applies to newly opened diffs.",
					"scope": "application"
				},
				"fittencode.indexRepository.enabled": {
					"type": "boolean",
					"default": false,
					"markdownDescription": "Enable the command to index your repository and give more context to the AI model. Experimental.",
					"scope": "application"
				},
				"fittencode.action.editCode.showInEditorContextMenu": {
					"type": "boolean",
					"default": true,
					"markdownDescription": "Show this action in the editor context menu, when you right-click on the code.",
					"scope": "application"
				},
				"fittencode.action.startChat.showInEditorContextMenu": {
					"type": "boolean",
					"default": true,
					"markdownDescription": "Show this action in the editor context menu, when you right-click on the code.",
					"scope": "application"
				},
				"fittencode.action.explainCode.showInEditorContextMenu": {
					"type": "boolean",
					"default": true,
					"markdownDescription": "Show this action in the editor context menu, when you right-click on the code.",
					"scope": "application"
				},
				"fittencode.action.findBugs.showInEditorContextMenu": {
					"type": "boolean",
					"default": true,
					"markdownDescription": "Show this action in the editor context menu, when you right-click on the code.",
					"scope": "application"
				},
				"fittencode.action.generateUnitTest.showInEditorContextMenu": {
					"type": "boolean",
					"default": true,
					"markdownDescription": "Show this action in the editor context menu, when you right-click on the code.",
					"scope": "application"
				},
				"fittencode.action.documentCode.showInEditorContextMenu": {
					"type": "boolean",
					"default": true,
					"markdownDescription": "Show this action in the editor context menu, when you right-click on the code.",
					"scope": "application"
				},
				"fittencode.action.startCustomChat.showInEditorContextMenu": {
					"type": "boolean",
					"default": true,
					"markdownDescription": "Show this action in the editor context menu, when you right-click on the code.",
					"scope": "application"
				}
			}
		},
		"keybindings": [
			{
				"key": "Ctrl+down",
				"command": "editor.action.inlineSuggest.acceptNextLine",
				"when": "inlineSuggestionVisible"
			},
			{
				"command": "fittencode.startChat",
				"when": "isMac",
				"key": "Ctrl+Cmd+c"
			},
			{
				"command": "fittencode.startChat",
				"when": "!isMac",
				"key": "Ctrl+Alt+c"
			},
			{
				"command": "fittencode.editCode",
				"when": "isMac",
				"key": "Ctrl+Cmd+e"
			},
			{
				"command": "fittencode.editCode",
				"when": "!isMac",
				"key": "Ctrl+Alt+e"
			},
			{
				"command": "fittencode.generateCode",
				"when": "isMac",
				"key": "Ctrl+Cmd+g"
			},
			{
				"command": "fittencode.generateCode",
				"when": "!isMac",
				"key": "Ctrl+Alt+g"
			}
		],
		"menus": {
			"view/title": [
				{
					"command": "fittencode.startChat",
					"when": "view == fittencode.chat",
					"group": "navigation@1"
				},
				{
					"command": "fittencode.editCode",
					"when": "view == fittencode.chat",
					"group": "navigation@2"
				},
				{
					"command": "fittencode.generateCode",
					"when": "view == fittencode.chat",
					"group": "navigation@3"
				},
				{
					"command": "fittencode.getStarted",
					"when": "view == fittencode.chat",
					"group": "navigation@9"
				}
			],
			"touchBar": [
				{
					"command": "fittencode.touchBar.startChat",
					"group": "fittencode"
				}
			],
			"commandPalette": [
				{
					"command": "fittencode.touchBar.startChat",
					"when": "false"
				}
			],
			"editor/context": [
				{
					"command": "fittencode.startChat",
					"group": "fittencode",
					"when": "config.fittencode.action.startChat.showInEditorContextMenu && editorHasSelection"
				},
				{
					"command": "fittencode.editCode",
					"group": "fittencode",
					"when": "config.fittencode.action.editCode.showInEditorContextMenu && editorHasSelection"
				},
				{
					"command": "fittencode.explainCode",
					"group": "fittencode",
					"when": "config.fittencode.action.explainCode.showInEditorContextMenu && editorHasSelection"
				},
				{
					"command": "fittencode.findBugs",
					"group": "fittencode",
					"when": "config.fittencode.action.findBugs.showInEditorContextMenu && editorHasSelection"
				},
				{
					"command": "fittencode.generateUnitTest",
					"group": "fittencode",
					"when": "config.fittencode.action.generateUnitTest.showInEditorContextMenu && editorHasSelection"
				},
				{
					"command": "fittencode.documentCode",
					"group": "fittencode",
					"when": "config.fittencode.action.documentCode.showInEditorContextMenu && editorHasSelection"
				}
			]
		},
